                "logout_icon": "\udb81\uddfd",
                "logout_icon_size": "20px",
                "reboot_icon": "\udb81\udc53",
                "reboot_icon_size": "20px",
                "shutdown_icon": "\uf011",
                "shutdown_icon_size": "20px"
