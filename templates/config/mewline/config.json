            "compact": {
                "window_titles": {
                    "enable_icon": true,
                    "truncation": true,
                    "truncation_size": 50,
                    "title_map": []
                },
                "music": {
                    "enabled": true,
                    "truncation": true,
                    "truncation_size": 30,
                    "default_album_logo": "https://sonos-partner-documentation.s3.amazonaws.com/ReadMe-External/content-service-features/add-images/add-album-art/SonosApp-DefaultArt-Alone.png"
